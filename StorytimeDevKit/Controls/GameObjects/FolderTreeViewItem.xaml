<UserControl x:Class="StoryTimeDevKit.Controls.GameObjects.FolderTreeViewItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="131">
    <TreeViewItem Name="TVItem" Padding="0" Margin="0,3,0,0">
        <TreeViewItem.Header>
            <StackPanel Orientation="Horizontal">
                <Image Height="20" Source="../../Images/GameObjectsControl/FolderTreeViewIcon.png" Width="20" Margin="0,0,2,0" />
                <TextBlock Name="Name" FontSize="14" />
            </StackPanel>
        </TreeViewItem.Header>
    </TreeViewItem>
</UserControl>
